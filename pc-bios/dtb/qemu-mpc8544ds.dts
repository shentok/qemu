/dts-v1/;

/ {
	compatible = "MPC8544DS", "MPC85xxDS";
	model = "MPC8544DS";
	#size-cells = <0x02>;
	#address-cells = <0x02>;

	platform@ff800000 {
		interrupt-parent = <0x8002>;
		ranges = <0x00 0x00 0xff800000 0x800000>;
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		compatible = "qemu,platform", "simple-bus";
	};

	pci@e0008000 {
		#address-cells = <0x03>;
		#size-cells = <0x02>;
		#interrupt-cells = <0x01>;
		clock-frequency = <0x3f940aa>;
		reg = <0x00 0xe0008000 0x00 0x1000>;
		ranges = <0x2000000 0x00 0xc0000000 0x00 0xc0000000 0x00 0x20000000 0x1000000 0x00 0x00 0x00 0xe1000000 0x00 0x10000>;
		fsl,msi = <0x8003>;
		bus-range = <0x00 0xff>;
		interrupts = <0x18 0x02>;
		interrupt-parent = <0x8002>;
		interrupt-map = <0x8800 0x00 0x00 0x01 0x8002 0x02 0x01 0x8800 0x00 0x00 0x02 0x8002 0x03 0x01 0x8800 0x00 0x00 0x03 0x8002 0x04 0x01 0x8800 0x00 0x00 0x04 0x8002 0x01 0x01 0x9000 0x00 0x00 0x01 0x8002 0x03 0x01 0x9000 0x00 0x00 0x02 0x8002 0x04 0x01 0x9000 0x00 0x00 0x03 0x8002 0x01 0x01 0x9000 0x00 0x00 0x04 0x8002 0x02 0x01>;
		interrupt-map-mask = <0xf800 0x00 0x00 0x07>;
		device_type = "pci";
		compatible = "fsl,mpc8540-pci";
		cell-index = <0x00>;
	};

	soc@e0000000 {
		bus-frequency = <0x00>;
		ranges = <0x00 0x00 0xe0000000 0x100000>;
		#size-cells = <0x01>;
		#address-cells = <0x01>;
		compatible = "fsl,mpc8544-immr", "simple-bus";
		device_type = "soc";

		msi@41600 {
			linux,phandle = <0x8003>;
			phandle = <0x8003>;
			interrupts = <0xe0 0x00 0xe1 0x00 0xe2 0x00 0xe3 0x00 0xe4 0x00 0xe5 0x00 0xe6 0x00 0xe7 0x00>;
			interrupt-parent = <0x8002>;
			msi-available-ranges = <0x00 0x100>;
			reg = <0x41600 0x200>;
			compatible = "fsl,mpic-msi";
		};

		global-utilities@e0000 {
			fsl,has-rstcr;
			reg = <0xe0000 0x1000>;
			compatible = "fsl,mpc8544-guts";
		};

		i2c@3000 {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			interrupt-parent = <0x8002>;
			interrupts = <0x2b 0x02>;
			cell-index = <0x00>;
			reg = <0x3000 0x14>;
			compatible = "fsl-i2c";
			device_type = "i2c";

			rtc@68 {
				reg = <0x68>;
				compatible = "pericom,pt7c4338";
			};
		};

		serial@4500 {
			interrupt-parent = <0x8002>;
			interrupts = <0x2a 0x02>;
			clock-frequency = <0x17d78400>;
			cell-index = <0x00>;
			reg = <0x4500 0x100>;
			compatible = "ns16550";
			device_type = "serial";
		};

		pic@40000 {
			interrupt-controller;
			linux,phandle = <0x8002>;
			phandle = <0x8002>;
			#interrupt-cells = <0x02>;
			#address-cells = <0x00>;
			reg = <0x40000 0x40000>;
			compatible = "fsl,mpic";
			device_type = "open-pic";
		};
	};

	aliases {
		pci0 = "/pci@e0008000";
		rtc = "/soc@e0000000/i2c@3000/rtc@68";
		i2c = "/soc@e0000000/i2c@3000";
		serial0 = "/soc@e0000000/serial@4500";
	};

	cpus {
		#size-cells = <0x00>;
		#address-cells = <0x01>;

		PowerPC,8544@0 {
			status = "okay";
			bus-frequency = <0x00>;
			i-cache-size = <0x8000>;
			d-cache-size = <0x8000>;
			i-cache-line-size = <0x20>;
			d-cache-line-size = <0x20>;
			reg = <0x00>;
			device_type = "cpu";
			timebase-frequency = <0x17d78400>;
			clock-frequency = <0x17d78400>;
		};
	};

	chosen {
		stdout-path = "/soc@e0000000/serial@4500";
		linux,stdout-path = "/soc@e0000000/serial@4500";
		rng-seed = <0xa39348fc 0xc917861f 0xa2fa4e37 0x20340f9c 0x6bf5185f 0xd1923193 0x5303b054 0xfa3caa70>;
		bootargs = [00];
		qemu,boot-kernel = <0x00 0x00 0x00 0x00>;
	};

	memory {
		reg = <0x00 0x00 0x00 0x8000000>;
		device_type = "memory";
	};
};
